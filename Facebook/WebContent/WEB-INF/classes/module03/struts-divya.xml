"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="divya" namespace="/module03" extends="tiles-default">
		<!-- All action Mappings will go over here -->
		<action name="redirectLogin" class="iiitb.fb.actions.Success"
			method="execute">
			<result name="success" type="tiles">loginPage</result>
			<result name="error" type="tiles">error</result>
		</action>
	
	<action name="Call_To_geteductaiondata" class="iiitb.fb.actions.EditProfileAction" method="geteducationdetails">
			<result name="success" type="tiles">EditProfileDisplay</result>
			<result name="error" type="tiles">error</result>
		</action>
	
	<action name="Call_To_SaveEditPageData" class="iiitb.fb.actions.SaveEditPageData" method="execute">
			<result name="success" type="tiles">EditProfileDisplay</result>
			<result name="error" type="tiles">error</result>
		</action>
	
	</package>
</struts>